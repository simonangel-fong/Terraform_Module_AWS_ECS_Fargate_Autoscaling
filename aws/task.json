[
  {
    "name": "fastapi",
    "image": "simonangelfong/demo-fargate-app",
    "cpu": 256,
    "memory": 512,
    "networkMode": "awsvpc",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/aws/ecs/demo-ecs-autoscaling",
        "awslogs-region": "ca-central-1",
        "awslogs-stream-prefix": "fastapi"
      }
    }
  }
]
